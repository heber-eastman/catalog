{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API server",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "build": "echo 'Backend build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:smoke": "jest __tests__/smoke/smoke.test.js --testTimeout=30000",
    "test:e2e": "jest __tests__/e2e/ --testTimeout=60000 --runInBand",
    "test:e2e:localstack": "docker-compose up -d localstack && npm run test:e2e && docker-compose down localstack",
    "lint": "eslint .",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "backup:db": "pg_dump $DATABASE_URL > backup_$(date +%Y%m%d_%H%M%S).sql",
    "restore:db": "psql $DATABASE_URL < $BACKUP_FILE",
    "security:audit": "npm audit --audit-level moderate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.839.0",
    "@aws-sdk/client-sqs": "^3.839.0",
    "aws-sdk": "^2.1565.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.0.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.1.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.1",
    "slugify": "^1.6.6",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@testcontainers/localstack": "^11.0.3",
    "@types/jest": "^29.5.0",
    "aws-sdk-client-mock": "^4.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "umzug": "^2.3.0"
  }
}
