{
    "family": "catalog-golf-backend",
    "taskRoleArn": "arn:aws:iam::722895251763:role/ecsTaskExecutionRole-catalog-golf",
    "executionRoleArn": "arn:aws:iam::722895251763:role/ecsTaskExecutionRole-catalog-golf",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "name": "catalog-golf-backend",
            "image": "722895251763.dkr.ecr.us-east-1.amazonaws.com/catalog-golf-backend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PORT",
                    "value": "3000"
                },
                {
                    "name": "NODE_ENV",
                    "value": "production"
                }
            ],
            "secrets": [
                {
                    "name": "DATABASE_URL",
                    "valueFrom": "arn:aws:ssm:us-east-1:722895251763:parameter/catalog-golf/database-url"
                },
                {
                    "name": "JWT_SECRET",
                    "valueFrom": "arn:aws:ssm:us-east-1:722895251763:parameter/catalog-golf/jwt-secret"
                },
                {
                    "name": "EMAIL_QUEUE_URL",
                    "valueFrom": "arn:aws:ssm:us-east-1:722895251763:parameter/catalog-golf/email-queue-url"
                },
                {
                    "name": "AWS_REGION",
                    "valueFrom": "arn:aws:ssm:us-east-1:722895251763:parameter/catalog-golf/aws-region"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/catalog-golf-backend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
} 