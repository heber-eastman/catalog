{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Vue 3 frontend with Vuetify",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "dev:5174": "vite --port 5174 --strictPort",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:v2": "cypress run --spec cypress/e2e/settings.v2.cy.js,cypress/e2e/settings.v2.overrides-crud.cy.js,cypress/e2e/settings.v2.seasons-crud.cy.js,cypress/e2e/settings.v2.templates-crud.cy.js,cypress/e2e/settings.v2.calendar-range.cy.js",
    "cypress:run:a11y": "cypress run --spec cypress/e2e/settings.v2.a11y.cy.js",
    "test:e2e": "start-server-and-test dev http://localhost:5173 cypress:run",
    "test:e2e:open": "start-server-and-test dev http://localhost:5173 cypress:open",
    "test:e2e:v2": "start-server-and-test dev http://localhost:5173 \"cypress run --spec cypress/e2e/settings.v2.cy.js,cypress/e2e/settings.v2.overrides-crud.cy.js,cypress/e2e/settings.v2.seasons-crud.cy.js,cypress/e2e/settings.v2.templates-crud.cy.js,cypress/e2e/settings.v2.calendar-range.cy.js\"",
    "test:e2e:a11y": "start-server-and-test dev http://localhost:5173 cypress:run:a11y",
    "test:e2e:ci": "cypress run --browser chrome --headless",
    "test:e2e:staging": "cypress run --env baseUrl=https://staging.catalog.golf",
    "test:e2e:prod": "cypress run --env baseUrl=https://catalog.golf",
    "performance:test": "lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html",
    "security:audit": "npm audit --audit-level moderate"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "axios": "^1.4.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.3.15"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "axe-core": "^4.10.3",
    "cypress": "^13.15.1",
    "cypress-axe": "^1.7.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.5.3",
    "sass": "^1.64.1",
    "start-server-and-test": "^2.0.12",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.0.3",
    "vitest": "^3.2.3"
  }
}
